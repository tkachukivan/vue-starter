{
  "name": "vue_exemple",
  "description": "'simle app with vue.js and express.js'",
  "version": "1.0.0",
  "author": "Ivan Tkachuk <tkachukivan93@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "run-server": "node ./server/bin/www.js",
    "run-server-dev": "nodemon ./server/bin/www.js",
    "run-server-pm2": "pm2 start ./server/config/pm2.config.json",
    "run-server-pm2-docker": "pm2 start ./server/config/pm2.config.docker.json",
    "stop-pm2": "pm2 stop books-app",
    "lint": "eslint ./server ./client -c ./.eslintrc"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "debug": "^4.1.0",
    "express": "^4.15.2",
    "mongoose": "^4.9.3",
    "morgan": "^1.9.1",
    "nconf": "^0.8.4",
    "nodemon": "^1.18.4",
    "promise-polyfill": "^6.0.2",
    "vue": "^2.2.1",
    "vue-router": "^2.3.0",
    "vuex": "^2.2.1",
    "vuex-router-sync": "^4.1.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.14.0",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
